{
  "manifest_version": 3,
  "name": "Focus View Extension",
  "version": "1.0.0",
  "description": "Enhance your video viewing experience",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}